

#include <iostream>
int main(void)
{
    std::cout<<"Size of char:"<<sizeof(char);
    std::cout<<"\n";
    std::cout<<"Size of int:"<<sizeof(int);
    std::cout<<"\n";
    std::cout<<"Size of float:"<<sizeof(float);
    std::cout<<"\n";
    std::cout<<"Size of double:"<<sizeof(double);
    std::cout<<"\n";
}
